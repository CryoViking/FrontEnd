{
  "name": "beam",
  "version": "1.3.0",
  "description": "",
  "main": "public/electron.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "electron-start": "electron .",
    "preelectron-pack-mac": "npm run build &&  codesign --deep --force --verbose --sign \"E943CCEAFA673A8EB00A7BD86F44E91047190661\" resources/main",
    "preelectron-pack-win": "npm run build",
    "electron-pack-mac": "electron-builder -c.extraMetadata.main=build/electron.js -p always  --mac",
    "electron-pack-win": "electron-builder -c.extraMetadata.main=build/electron.js -p always  --win "
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "bootstrap": "^4.5.2",
    "electron-is-dev": "^1.2.0",
    "mkdirp": "^1.0.4",
    "notistack": "^1.0.1",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "electron": "^9.1.2",
    "electron-builder": "^22.9.1",
    "electron-notarize": "^1.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "build": {
    "win": {
      "target": [
        {
          "target": "nsis"
        },
        {
          "target": "zip"
        }
      ],
      "publish": [
        {
          "provider": "github",
          "owner": "doalier",
          "repo": "Frontend",
          "releaseType": "release"
        }
      ],
      "certificateSubjectName": "ESABUNOR NUKIE, TEGA AGHOGHO"
    },
    "mac": {
      "target": [
        {
          "target": "dmg"
        },
        {
          "target": "zip"
        }
      ],
      "appId": "com.esabunor.frontend",
      "forceCodeSigning": true,
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "category": "public.app-category.productivity",
      "entitlements": "entitlements.mac.plist",
      "provisioningProfile": "embedded.provisionprofile",
      "publish": [
        {
          "provider": "github",
          "owner": "CryosisOS",
          "repo": "Frontend",
          "releaseType": "release"
        }
      ]
    },
    "afterSign": "scripts/notarize.js",
    "dmg": {
      "sign": false
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    },
    "extends": null,
    "extraFiles": [
      {
        "from": "resources/",
        "to": "resources/",
        "filter": [
          "**/*"
        ]
      }
    ]
  }
}
